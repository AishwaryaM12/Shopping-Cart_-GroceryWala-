<div class="row">
<div class="col-sm-6 col-md-4 col-md-offset-4 col-sm-offset-3">
        <div id="success" class="alert alert-success" {{#if noMessages}}hidden{{/if}}>
            {{ successMsg }}
        </div>
    </div>
</div>
<h3>Grocery and staples <img style="height:80px; width:80px;" src="/images/staples.jpeg"></h3>
{{#each products}}
<div class="row">
	{{#each this}}
	{{#ifEquals 'Grocery and staples' this.StockName}}
	<div class="col-sm-6 col-md-4">
        <div class="thumbnail">
            <img src="{{ this.ImagePath }}" alt="{{ this.title }}" class="img-responsive">
            <div class="caption">
                <h3>{{this.title}}</h3>
                <p class="description">{{this.weight}}Kg</p>
                <div class="clearfix">
                    <div class="price pull-left">Rs {{this.price}} </div>
                    <a href="/add-to-cart/{{this._id}}" class="btn btn-success pull-right" role="button">Add to Cart</a>
                </div>
            </div>
        </div>
    </div>
    {{/ifEquals}}
    {{/each}}
</div> 
{{/each}}


